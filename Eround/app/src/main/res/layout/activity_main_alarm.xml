<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#66000000"
    android:orientation="vertical"
    android:paddingTop="55dp"
    tools:context=".Main.Alarm">

    <FrameLayout
        android:id="@+id/activity_main_alarm_id_content_1"
        android:layout_width="match_parent"
        android:layout_height="100dp"
        android:layout_marginTop="5dp">

        <ImageView
            android:id="@+id/activity_main_alarm_id_tag_background_1"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="#444444"
            android:contentDescription="@string/tagBackGround_hashFragment"
            android:scaleType="centerCrop"
            android:tint="#99000000" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center"
            android:orientation="vertical">

            <TextView
                android:id="@+id/activity_main_alarm_id_tag_content1_1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:text="The Lounge : 내일이 세상의 마지막 날이라면?"
                android:textColor="#FFFFFF"
                android:textSize="18sp" />

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/activity_main_alarm_id_tag_content2_1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:text="라운지가 열렸습니다!"
                    android:textColor="#FFFFFF" />
            </LinearLayout>
        </LinearLayout>
    </FrameLayout>

    <FrameLayout
        android:id="@+id/activity_main_alarm_id_content_2"
        android:layout_width="match_parent"
        android:layout_height="100dp"
        android:layout_marginTop="5dp">

        <ImageView
            android:id="@+id/activity_main_alarm_id_tag_background_2"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:contentDescription="@string/tagBackGround_hashFragment"
            android:scaleType="centerCrop"
            android:tint="#99000000"
            android:background="#444444" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center"
            android:orientation="vertical">

            <TextView
                android:id="@+id/activity_main_alarm_id_tag_content1_2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:text="The Lounge : 죽기전에 이것만은 꼭 해보고 싶어요."
                android:textColor="#FFFFFF"
                android:textSize="18sp" />

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/activity_main_alarm_id_tag_content2_2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:text=""
                    android:textColor="#FFFFFF" />
            </LinearLayout>
        </LinearLayout>
    </FrameLayout>

    <FrameLayout
        android:id="@+id/activity_main_alarm_id_content_3"
        android:layout_width="match_parent"
        android:layout_height="100dp"
        android:layout_marginTop="5dp">

        <ImageView
            android:id="@+id/activity_main_alarm_id_tag_background_3"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:contentDescription="@string/tagBackGround_hashFragment"
            android:scaleType="centerCrop"
            android:tint="#99000000"
            android:background="#444444" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center"
            android:orientation="vertical">

            <TextView
                android:id="@+id/activity_main_alarm_id_tag_content1_3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:text="The Lounge : 본격 셀프 자랑"
                android:textColor="#FFFFFF"
                android:textSize="18sp" />

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/activity_main_alarm_id_tag_content2_3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:text="라운지가 열렸습니다!"
                    android:textColor="#FFFFFF" />
            </LinearLayout>
        </LinearLayout>
    </FrameLayout>


</LinearLayout>